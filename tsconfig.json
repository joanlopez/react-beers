{
  "compilerOptions": {
    "baseUrl": "http://localhost:3000/",
    "outDir": "build/dist",
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "inlineSourceMap": false,
    "inlineSources": false,
    "jsx": "react",
    "moduleResolution": "node",
    "strictNullChecks": true,
    "noImplicitAny": false
  },
  "exclude": [
    "node_modules",
    "build",
    "scripts",
    "acceptance-tests",
    "webpack",
    "jest",
    "src/setupTests.ts"
  ]
}
